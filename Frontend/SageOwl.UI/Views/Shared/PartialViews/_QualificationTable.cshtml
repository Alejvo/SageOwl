@model QualificationViewModel

<table>
    <thead>
        <tr>
            <th>Name</th>
            @for (int i = 0; i < Model.Descriptions.Count; i++)
            {
                <th>@Model.Descriptions[i]</th>
            }
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.UserQualifications.Count; i++)
        {
            <tr>
                <td>@Model.UserQualifications[i].Name</td>
                @for (int j = 0; j < Model.UserQualifications[i].Grades.Count; j++)
                {
                    <td>@Model.UserQualifications[i].Grades[j]</td>
                }
            </tr>
        }
    </tbody>
</table>
